<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="concurs.model">
    <class name="Concurs" table="Concurs">
        <id name="id" column="IDCursa">
            <generator class="increment"></generator>
        </id>

        <property name="capacitate" column="Capacitate"/>
        <property name="distanta" column="Distanta"/>
        <property name="nume" column="Nume"/>
        <property name="numar" column="Numar"/>
    </class>
</hibernate-mapping>